@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Vacancies Parse</h1>
  
    <form method="post" action="~/Home/Parse">
        <input name="url" placeholder="Enter Url" />
        <input type="submit" class="btn btn-primary btn-default"  />      
    </form>
</div>

        <table class="table">
            <tr>
                <th>
                   Salary
                </th>
                <th>
                   Description
                </th>
                <th>
                    City
                </th>
                <th>
                    Country
                </th>
                <th>
                    Vacancy
                </th>
                <th></th>
            </tr>

            @foreach (var item in ViewBag.Vacancies) {
                <tr>
                    <td>
                       @item.Salary
                    </td>
                    <td>
                        @item.Description
                    </td>
                    <td>
                        @item.City
                    </td>
                    <td>
                        @item.Country
                    </td>
                    <td>
                        @item.VacancyName
                    </td>
                  
                </tr>
            }

        </table>
      
